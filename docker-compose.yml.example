version: '3.8'
services:
  irc-logger:
    build: .
    ports:
      - "8080:8080"
    environment:
      IRC_CHANNEL: "#YourChannel"
      IRC_SERVER: "irc.byteirc.org"
      IRC_NICKNAME: "test"
      IRC_USERNAME: "User"
      IRC_NAME: "logger bot"
      IRC_TIMEZONE: "Europe/London"
      HTTP_USERNAME: "yourUsername"
      HTTP_PASSWORD: "yourPassword"
      HTTP_PORT: "8080"
